%p#notice= notice
%p
  %strong Name:
  = @season.name
%p
  %strong Beginning date:
  = @season.beginning_date
%p
  %strong Ending date:
  = @season.ending_date
%div
  %ul
    - @season.memberships.each do |membership|
      %li{id: "#{membership.month.name.downcase}_membership"}
        = membership.month
        = membership.member.try(:name)

= link_to 'Alter Season', edit_season_path(@season)
|
= link_to 'Back', seasons_path
